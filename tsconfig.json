{
  "extends": "@calcom/tsconfig/base.json",
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": "./",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "jsx": "preserve"
  },
  "include": [
    "./pages/api/*.ts"
  ]
}
