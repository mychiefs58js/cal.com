{
  "extends": "@calcom/tsconfig/nextjs.json",
  "compilerOptions": {
    "target": "es2017",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@api/*": ["pages/api/*"],
      "@lib/*": ["lib/*"],
      "@/*": ["*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules", "templates", "auth"]
}
